{
  "name": "react-avatar-crop",
  "version": "0.0.2",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run lint && npm run build:library && npm run build:storybook",
    "build:library": "rimraf build && tsc && npm run copy-css-to-lib && npm run copy-svg-to-lib && npm run copy-png-to-lib && npm run copy-woff2-to-lib",
    "build:storybook": "build-storybook -s public",
    "build:watch": "rimraf build && tsc --watch",
    "test": "jest",
    "publish": "npm run publish:docs",
    "publish:docs": "storybook-to-ghpages -e storybook-static",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build",
    "copy-woff2-to-lib": "cpx \"./src/**/*.woff2\" ./build",
    "copy-svg-to-lib": "cpx \"./src/**/*.svg\" ./build",
    "copy-png-to-lib": "cpx \"./src/**/*.png\" ./build",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.13",
    "@storybook/storybook-deployer": "^2.8.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.0",
    "@types/testing-library__jest-dom": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jest-dom": "^2.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "jest": "^25.1.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "react-easy-crop": "^2.1.0",
    "styled-components": "^5.0.1"
  }
}
